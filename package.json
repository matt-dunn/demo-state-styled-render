{
  "name": "demo-state-styled-render",
  "version": "1.0.0",
  "description": "Pure TypeScript implementation of styled JSX functional components and state management",
  "main": "public/index.html",
  "repository": "git@github.com:matt-dunn/demo-state-styled-render.git",
  "author": "Matt Dunn <matt.j.dunn@gmail.com>",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint:css": "yarn stylelint \"./src/**/*.{css,scss,sass,jsx,tsx}\" --fix",
    "lint:js": "eslint ./ --ext .js,.ts,.jsx,.tsx --fix",
    "lint": "yarn lint:css && yarn lint:js",
    "start": "parcel public/index.html --https",
    "prebuild": "yarn lint && yarn type-check && yarn test",
    "build": "rimraf dist && parcel build public/index.html --public-url ./ --out-dir ./dist --no-cache",
    "serve": "npx http-server dist/ -g -b -S -C ./etc/cert.pem -K ./etc/key.pem",
    "bundle": "node ./etc/scripts/bundle",
    "test:unit": "jest --passWithNoTests",
    "test": "yarn test:unit --coverage",
    "ci": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@types/jsdom": "^16.2.4",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.0.1",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.6.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "^3.6.5",
    "del": "^6.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "jest-junit": "^12.0.0",
    "jsdom": "^16.4.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-compress": "^2.0.2",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-modules": "^3.2.2",
    "posthtml": "^0.13.3",
    "prettier": "^2.1.2",
    "recursive-copy": "^2.0.11",
    "sass": "^1.26.8",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "stylis": "^3.5.4"
  },
  "compress": {
    "test": ".(js|html|css|xml|webmanigest)$",
    "threshold": 1000,
    "concurrency": 2,
    "gzip": {
      "enabled": true,
      "numiterations": 15,
      "blocksplitting": true,
      "blocksplittinglast": false,
      "blocksplittingmax": 15,
      "zlib": false,
      "zlibLevel": 9,
      "zlibMemLevel": 9
    },
    "brotli": {
      "enabled": true,
      "mode": 0,
      "quality": 11,
      "lgwin": 24,
      "enable_context_modeling": true
    },
    "compressOutput": true
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.22"
  }
}
